<body class="text-center">
  <form class="form-signin" action="" method="POST">
    <% if (loginMessage.length > 0) {%>
    <div class="alert alert-danger" role="alert">
      <%= loginMessage %>
    </div>
    <% } %>
    <img class="mb-4" src="/images/computerwindows.svg" alt="" width="200" height="200">
    <h1 class="h3 mb-3 font-weight-normal" style="font-family: 'Dosis';">Project Management</h1>
    <label for="inputEmail" class="sr-only">Email address</label>
    <input type="email" id="inputEmail" name="email" class="form-control" placeholder="Email address" required=""
      autofocus="">
    <label for="inputPassword" class="sr-only">Password</label>
    <input type="password" id="inputPassword" name="password" class="form-control" placeholder="Password" required="">
    <div class="checkbox mb-3">
      <label>
        <input type="checkbox" value="remember-me"> Remember me
      </label>
    </div>
    <button class="btn btn-lg btn-block" type="submit" style="background-color: #321a75; color: #fff;">Sign in</button>
    <p class="mt-5 mb-3 text-muted">
      Made with <i class="fa fa-heart-o"></i> by <a href="https://www.linkedin.com/in/pisckipratama"
        target="_blank">Piscki Pratama</a>
    </p>
  </form>


</body>

